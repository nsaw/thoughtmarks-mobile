{
  "name": "tm-mobile-cursor",
  "version": "1.0.0",
  "description": "Thoughtmarks Mobile App",
  "scripts": {
    "logs:cleanup": "node scripts/log-manager.js cleanup",
    "logs:rotate": "node scripts/log-manager.js rotate",
    "logs:stats": "node scripts/log-manager.js stats",
    "logs:auto-cleanup": "node scripts/log-manager.js cleanup && node scripts/log-manager.js rotate",
    "predev": "npm run logs:cleanup",
    "prebuild": "npm run logs:cleanup",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:guard": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix-all": "eslint . --ext .ts,.tsx,.js,.jsx --fix && prettier --write .",
    "autolint:start": "node scripts/autolinter.js start",
    "autolint:stop": "node scripts/autolinter.js stop",
    "autolint:scan": "node scripts/autolinter.js scan",
    "autolint:stats": "node scripts/autolinter.js stats",
    "lint:check": "npm run lint:guard && npx tsc --noEmit",
    "lint:fix-check": "npm run lint:fix-all && npm run lint:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:parallel": "jest --maxWorkers=4",
    "test:legacy": "EXPO_PUBLIC_USE_NEXTGEN=false jest",
    "test:nextgen": "EXPO_PUBLIC_USE_NEXTGEN=true jest",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.0.0",
    "chokidar": "^3.6.0",
    "detox": "^20.17.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-thoughtmarks": "file:eslint-plugin-thoughtmarks",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "modern-screenshot": "^4.6.5",
    "puppeteer": "^24.14.0",
    "ts-jest": "^29.1.1"
  },
  "dependencies": {
    "@expo-google-fonts/oswald": "^0.4.1",
    "@expo-google-fonts/ubuntu": "^0.4.0",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/styled-components": "^5.1.34",
    "@types/styled-components-react-native": "^5.2.5",
    "dotenv": "^17.2.0",
    "expo": "^53.0.20",
    "expo-apple-authentication": "^7.2.4",
    "expo-auth-session": "^6.2.1",
    "expo-av": "^15.1.7",
    "expo-blur": "^14.1.5",
    "expo-file-system": "^18.1.11",
    "expo-font": "^13.3.2",
    "expo-speech": "^13.1.7",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "^2.2.3",
    "expo-updates": "^0.28.17",
    "express": "^5.1.0",
    "firebase": "^12.0.0",
    "lucide-react-native": "^0.525.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0",
    "styled-components": "^6.1.19",
    "zustand": "^5.0.6"
  }
}
