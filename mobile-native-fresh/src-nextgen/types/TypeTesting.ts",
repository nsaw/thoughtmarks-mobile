import { User, Thoughtmark, Bin, Task } from './DataTypes';

export const typeTester = {
  createValidUser: (): User => ({
    id: 'user-1',
    email: 'test@example.com',
    name: 'Test User',
    createdAt: new Date().toISOString(),
    updatedAt: new Date().toISOString(),
  }),
  
  createValidThoughtmark: (): Thoughtmark => ({
    id: 'thoughtmark-1',
    content: 'Test thoughtmark content',
    userId: 'user-1',
    binId: 'bin-1',
    createdAt: new Date().toISOString(),
    updatedAt: new Date().toISOString(),
  }),
  
  createValidBin: (): Bin => ({
    id: 'bin-1',
    name: 'Test Bin',
    userId: 'user-1',
    createdAt: new Date().toISOString(),
    updatedAt: new Date().toISOString(),
  }),
  
  createValidTask: (): Task => ({
    id: 'task-1',
    title: 'Test Task',
    completed: false,
    userId: 'user-1',
    createdAt: new Date().toISOString(),
    updatedAt: new Date().toISOString(),
  }),
  
  createInvalidUser: () => ({
    id: 123, // Should be string
    email: null, // Should be string
    name: undefined, // Should be string
  }),
  
  createInvalidThoughtmark: () => ({
    id: null, // Should be string
    content: 123, // Should be string
    userId: undefined, // Should be string
  }),
  
  createInvalidBin: () => ({
    id: false, // Should be string
    name: 456, // Should be string
    userId: null, // Should be string
  }),
  
  createInvalidTask: () => ({
    id: undefined, // Should be string
    title: null, // Should be string
    completed: 'true', // Should be boolean
  }),
}; 