        "projectId": "thoughtmarks-25replit",
        "storageBucket": "thoughtmarks-25replit.appspot.com",
        "messagingSenderId": "371020564944",
        "appId": "1:371020564944:web:your-app-id"
      },
      "googleAndroidClientId": "482692133923-uahvc6eshmr3p03mv8dl9vgk6vrl6b5v.apps.googleusercontent.com",
      "googleIosClientId": "482692133923-urb0tq0gcjbp06e26gimulhjp18kpcpb.apps.googleusercontent.com"
    },
    "jsEngine": "jsc",
